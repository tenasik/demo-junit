{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: My gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"main.c",
				"fun.c",
				"-o",
				"program"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}